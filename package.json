{
  "name": "things-scene-wheel-sorter",
  "version": "2.0.0",
  "description": "An element providing a starting point for your own reusable Polymer elements.",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "serve": "watchify src/*.js -o things-scene-wheel-sorter.js src/index.js & polyserve",
    "test": "./node_modules/.bin/mocha --require should --compilers js:babel-core/register test/**/test-*.js",
    "build": "browserify src/index.js -o things-scene-wheel-sorter.js && uglifyjs things-scene-wheel-sorter.js -o things-scene-wheel-sorter-min.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ThingsElements/things-scene-wheel-sorter.git"
  },
  "author": "",
  "license": "http://things-scene.hatiolab.com/EULA/",
  "bugs": {
    "url": "https://github.com/ThingsElements/things-scene-wheel-sorter/issues"
  },
  "homepage": "https://github.com/ThingsElements/things-scene-wheel-sorter#readme",
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.26",
    "babel-preset-es2015": "^6.3.13",
    "babelify": "~7.2.0",
    "browserify": "~10.2.4",
    "chai": "^3.4.1",
    "mocha": "^2.3.4",
    "should": "^8.0.2",
    "uglifyjs": "^2.4.10",
    "watchify": "^3.3.1",
    "node-canvas": "Automattic/node-canvas"
  },
  "dependencies": {}
}
